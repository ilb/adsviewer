    "ajv-i18n": "^3.6.0",
    "await-timeout": "^1.1.1",
    "awilix": "^4.3.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "next": "^10.0.4",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^6.0.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-datepicker": "^3.7.0",
    "react-dom": "^17.0.1",
    "semantic-ui-offline": "^2.4.1-1",
    "semantic-ui-react": "^2.0.1",
