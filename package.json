    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "loadads": "node -r dotenv/config bin/loadads.mjs"
  },
  "dependencies": {
    "@ilb/uniformscomponents": "^1.0.3",
    "@ilb/uriaccessorjs": "^1.0.5",
