{
  "name": "adsviewer",
  "description": "Сервис просмотра объявлений",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon server.js",
    "next": "next",
    "build": "next build",
    "start": "next start",
    "prisma:push": "prisma db push",
    "ilb-build": "prisma:push && next build",
